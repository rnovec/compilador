{
  "name": "syntax-analyzer",
  "version": "1.0.0",
  "private": true,
  "description": "Analizador Léxico para la materia Lenguajes y Autómatas I",
  "author": "Raul Novelo Cruz <rnovelo.cruz98@gmail.com>",
  "scripts": {
    "dev": "quasar dev -m electron -t mat",
    "build": "quasar build -m electron",
    "build:win": "npm run build && npm run win32",
    "build:deb": "npm run build && npm run deb64",
    "win32": "electron-installer-windows --src dist/electron/SyntaxAnalyzer-win32-x64 --dest dist/installers --arch amd64 --config config.json",
    "deb64": "electron-installer-debian --src ./dist/electron/SyntaxAnalyzer-linux-x64/ --dest dist/installers/ --arch amd64 --config config.json"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "buefy": "^0.8.0",
    "esprima": "^4.0.1",
    "quasar": "^1.0.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "devtron": "^1.4.0",
    "electron": "^8.3.0",
    "electron-builder": "^22.6.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^14.2.1",
    "quasar-pak": "0.0.2",
    "vue-cli-plugin-buefy": "^0.3.7"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "capacitorId": "",
  "cordovaId": "org.cordova.quasar.app",
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "executableName": "SyntaxAnalyzer",
  "productName": "SyntaxAnalyzer"
}
